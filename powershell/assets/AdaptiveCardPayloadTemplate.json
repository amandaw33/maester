{
    "type": "AdaptiveCard",
    "body": [
        {
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": "auto",
                    "items": [
                        {
                            "type": "Image",
                            "url": "data:image/png;base64,",
                            "size": "Small"
                        }
                    ]
                },
                {
                    "type": "Column",
                    "width": "stretch",
                    "items": [
                        {
                            "type": "TextBlock",
                            "size": "Medium",
                            "weight": "Bolder",
                            "text": "${$root.title}"
                        },
                        {
                            "type": "TextBlock",
                            "text": "${$root.description}",
                            "wrap": true,
                            "size": "Small",
                            "isSubtle": true,
                            "spacing": "None"
                        }
                    ]
                }
            ]
        },
        {
            "type": "Container",
            "showBorder": true,
            "items": [
                {
                    "type": "ColumnSet",
                    "columns": [
                        {
                            "type": "Column",
                            "width": "auto",
                            "spacing": "None",
                            "horizontalAlignment": "Left",
                            "minHeight": "15px",
                            "verticalContentAlignment": "Center",
                            "rtl": false,
                            "items": [
                                {
                                    "type": "Image",
                                    "url": "data:image/svg+xml;base64,",
                                    "width": "16px",
                                    "backgroundColor": "#fdf1e5"
                                }
                            ]
                        },
                        {
                            "type": "Column",
                            "width": "stretch",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "${$root.run.TenantName} (${$root.run.TenantId})",
                                    "wrap": true,
                                    "spacing": "None",
                                    "horizontalAlignment": "Left",
                                    "size": "Small",
                                    "weight": "Default",
                                    "color": "Attention",
                                    "isSubtle": true,
                                    "fontType": "Default"
                                }
                            ],
                            "horizontalAlignment": "Left"
                        }
                    ],
                    "spacing": "ExtraSmall",
                    "horizontalAlignment": "Center",
                    "minHeight": "6px",
                    "bleed": true
                }
            ],
            "roundedCorners": true,
            "style": "emphasis"
        },
        {
            "type": "Container",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "Test summary",
                    "wrap": true,
                    "spacing": "Large",
                    "style": "heading",
                    "size": "Large",
                    "weight": "Bolder"
                },
                {
                    "title": "Test status",
                    "id": "resultdonut",
                    "colorSet": "categorical",
                    "type": "Chart.Donut",
                    "data": [
                        {
                            "color": "good",
                            "legend": "Passed",
                            "value": 99990
                        },
                        {
                            "color": "attention",
                            "legend": "Failed",
                            "value": 99991
                        }
                    ]
                },
                {
                    "type": "Container",
                    "style": "default",
                    "roundedCorners": true,
                    "items": [
                        {
                            "type": "ColumnSet",
                            "columns": [
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Total tests",
                                            "wrap": true,
                                            "isSubtle": true,
                                            "weight": "Lighter",
                                            "size": "Small",
                                            "fontType": "Default",
                                            "style": "columnHeader"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${$root.run.TotalCount}",
                                            "wrap": true,
                                            "weight": "Bolder",
                                            "size": "Large"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Passed",
                                            "wrap": true,
                                            "isSubtle": true,
                                            "weight": "Lighter",
                                            "size": "Small",
                                            "fontType": "Default",
                                            "style": "columnHeader"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${$root.run.PassedCount}",
                                            "wrap": true,
                                            "size": "Large",
                                            "weight": "Bolder"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Failed",
                                            "wrap": true,
                                            "isSubtle": true,
                                            "weight": "Lighter",
                                            "size": "Small",
                                            "fontType": "Default",
                                            "style": "columnHeader"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${$root.run.FailedCount}",
                                            "wrap": true,
                                            "size": "Large",
                                            "weight": "Bolder"
                                        }
                                    ]
                                },
                                {
                                    "type": "Column",
                                    "width": "stretch",
                                    "items": [
                                        {
                                            "type": "TextBlock",
                                            "text": "Not tested",
                                            "wrap": true,
                                            "isSubtle": true,
                                            "weight": "Lighter",
                                            "size": "Small",
                                            "fontType": "Default",
                                            "style": "columnHeader"
                                        },
                                        {
                                            "type": "TextBlock",
                                            "text": "${$root.run.NotRunCount}",
                                            "wrap": true,
                                            "size": "Large",
                                            "weight": "Bolder"
                                        }
                                    ]
                                }
                            ]
                        }
                    ],
                    "showBorder": true
                }
            ],
            "id": "test-summary",
            "style": "default"
        },
        {
            "type": "ActionSet",
            "actions": [
                {
                    "type": "Action.OpenUrl",
                    "title": "Full test results",
                    "url": "${$root.run.TestResultURL}",
                    "tooltip": "Full test results of the Maester run.",
                    "style": "positive",
                    "iconUrl": "data:image/svg+xml;base64,"
                }
            ],
            "separator": true,
            "id": "1"
        },
        {
            "type": "TextBlock",
            "text": "Maester ${$root.run.ModuleVersion}",
            "wrap": true,
            "spacing": "Large",
            "separator": true,
            "size": "Small",
            "weight": "Lighter",
            "isSubtle": true,
            "color": "Light"
        }
    ],
    "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "fallbackText": "Maester test results: ${$root.run.PassedCount} tests passed. ${$root.run.FailedCount} tests failed.",
    "speak": "Maester test results: ${$root.run.PassedCount} tests passed. ${$root.run.FailedCount} tests failed."
}